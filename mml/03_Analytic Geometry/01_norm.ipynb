   "metadata": {},
   "outputs": [],
   "source": [
    "# 3-4-5 triangle vector\n",
    "v = np.array([3, 4])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "4b3d4b3a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate its L2 Norm (length)\n",
    "v_length = np.linalg.norm(v)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "2952b1a3",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Vector v: [3 4]\n",
      "Length of v: 5.0\n"
     ]
    }
   ],
   "source": [
    "# Print results\n",
    "print(f\"Vector v: {v}\")\n",
    "print(f\"Length of v: {v_length}\")"
   ]
  }
